{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "directive-selector": [true, "attribute", "app", "camelCase"],
        "component-selector": [true, "element", "app", "kebab-case"],
        "max-line-length": [true, 120],
        "object-literal-sort-keys": false
    }
}
